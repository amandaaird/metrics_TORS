<librec-auto>
	<random-seed>202110</random-seed>
	<thread-count>1</thread-count>
	<data>
		<data-dir>../../../data</data-dir>
		<format>UIR</format>
		<data-file format="text">rating_5000_cls_hc_10core_iterative.csv</data-file>
	</data>

	
	<features>
		<appender-class>net.librec.data.convertor.appender.ItemFeatureAppender</appender-class>
		<item-feature-file>loan_feature_df_hc_10core.csv</item-feature-file>
		<protected-feature name="fea:loan_buck_5" type="fea:loan_buck_5">loan_buck_5</protected-feature>
	</features>

	<optimize>
		<iterations>50</iterations>
		<metric>ndcg_metric.py</metric>
	</optimize>

	
	<splitter>
		<model count="5">kcv</model>
		<save>true</save>
	</splitter>

	<alg>
		<class>bpr</class>
		<learn-rate>0.01</learn-rate>
		<learn-rate-max>0.1</learn-rate-max>
	<learn-rate-decay>1.0</learn-rate-decay>
		<bold-driver>true</bold-driver>
        <iterator-max>100</iterator-max>
		<user-reg var="true">0.325234427445336</user-reg><item-reg var="true">0.014167498705645367</item-reg><num-factors var="true" type="discrete">66</num-factors><early-stop>true</early-stop>
	</alg>
	
	<metric>
		<ranking>true</ranking>
		<list-size>50</list-size>
		<class>ndcg,psp</class>
		<protected-feature type="fea:loan_buck_5">loan_buck_5</protected-feature>

		<script src="system" lang="python3">
			<script-name>ndcg_metric.py</script-name>
			<param name="list_size">10</param>
		</script>

		<script src="system" lang="python3">
			<script-name>psp.py</script-name>
			<param name="list_size">10</param>
			<param name="protected_feature">loan_buck_5</param>
		</script>
	</metric>

	<rerank>
		<script lang="python3" src="system">
			<script-name>far_rerank.py</script-name>
			<param name="max_len">10</param>
			<param var="true" name="lambda">0.18787022189803257</param><param name="binary">False</param>
			<param name="protected_feature" type="fea:loan_buck_5">loan_buck_5</param>
		</script>
	</rerank>

	

	<post>
		<script lang="python3" src="system">
			<script-name>result_graphics.py</script-name>
			<param name="browser">true</param>
		</script>
		<script lang="python3" src="system">
			<script-name>results_to_csv.py</script-name>
			<param name="option">all</param>
		</script>
	</post>
<!--This configuration file was automatically generated by librec-auto. Editing may produce unpredictable results and is not recommended.--></librec-auto>
